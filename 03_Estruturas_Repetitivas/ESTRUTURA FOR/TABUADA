class tabuada{


 static void Main(string[] args){


  int T , soma, i ;

   Console.WriteLine("Deseja a tabuada para qual valor ");
   T = int.Parse(Console.ReadLine());

   soma = 0;

   for( i = 1; i <= 10; i++){
    soma = T * i;
    Console.WriteLine($"{T} x {i} = {soma}");

   }

   



   }
}
      